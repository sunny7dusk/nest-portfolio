    "@sanity/image-url": "^1.0.1",
    "@tabler/icons-react": "^2.47.0",
    "autoprefixer": "^10.4.5",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "framer-motion": "^6.3.3",
    "gsap": "^3.10.4",
    "lottie-react": "^2.3.1",
    "mini-svg-data-uri": "^1.4.4",
    "next": "12.1.5",
    "next-sanity": "^0.5.2",
    "postcss": "^8.4.12",
    "react": "^17.0.2",
    "react-collapsed": "^3.3.2",
    "react-dom": "^17.0.2",
